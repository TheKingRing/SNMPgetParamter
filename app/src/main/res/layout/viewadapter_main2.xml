<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_marginTop="10dp"
        android:id="@+id/checklayout2"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <com.mycomp.mrwang.snmpgetparamter.ui.SmoothcheckBoxforAdapter
            android:id="@+id/checkboxfor3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            />
        <TextView
            android:textSize="18sp"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/checkboxfor3"
            android:id="@+id/viewadaptertext4"
            android:textStyle="bold"
            android:textColor="@color/win8_blue"
            android:layout_width="70dp"
            android:layout_height="wrap_content" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_below="@+id/checklayout2"
        android:layout_marginTop="50dp"
        android:id="@+id/checklayout3"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <com.mycomp.mrwang.snmpgetparamter.ui.SmoothcheckBoxforAdapter
            android:id="@+id/checkboxfor4"
            android:layout_width="20dp"
            android:layout_height="20dp"
            />
        <TextView
            android:textSize="18sp"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/checkboxfor4"
            android:id="@+id/viewadaptertext5"
            android:textStyle="bold"
            android:textColor="@color/win8_blue"
            android:layout_width="70dp"
            android:layout_height="wrap_content" />
    </RelativeLayout>

</RelativeLayout>